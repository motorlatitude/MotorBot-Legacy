{
  "manifest_version": 2,
  "name": "__MSG_extName__",
  "description": "This extension lets you add youtube videos to Motorbot",
  "version": "1.5.5",
  "version_name": "build 1.5.5~0907201901",
  "short_name": "MB",
  "default_locale": "en",
  "author": "Lennart Hase",
  "key": "",
  "browser_action": {
    "default_icon": "motorbotIcon.png",
    "default_popup": "popup.html",
    "default_title": "Motorbot"
  },
  "background": {
    "scripts": [
      "bg.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.youtube.com/*",
        "https://youtube.com/*",
        "https://motorbot.io/*",
        "https://soundcloud.com/*"
      ],
      "js": [
        "jquery.min.js",
        "ytb.js"
      ],
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources": [
    "icon_20x20.png",
    "icon.png"
  ],
  "oauth2": {
    "client_id": "169554794376200192",
    "scopes": [
      "identify email guilds"
    ]
  },
  "permissions": [
    "activeTab",
    "contextMenus",
    "tabs",
    "storage",
    "identity",
    "webRequest",
    "https://www.youtube.com/*",
    "*://*.youtube.com/*",
    "*://*.googlevideo.com/*",
    "https://youtube.com/*",
    "https://soundcloud.com/*",
    "https://discordapp.com/api/oauth2/*",
    "https://motorbot.io/api/oauth2/*"
  ],
  "icons": {
    "128": "motorbotIcon.png"
  }
}
