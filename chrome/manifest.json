{
  "manifest_version": 2,
  "name": "MotorBot",
  "description": "This extension will let you add youtube videos to MotorBot",
  "version": "1.0.6",
  "browser_action": {
   "default_icon": "icon.png",
   "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["https://www.youtube.com/*", "https://youtube.com/*"],
      "js": ["popup.js"],
      "run_at": "document_end"
    }
  ],
 "web_accessible_resources": [
     "icon.png"
   ],
  "permissions": [
   "activeTab",
   "contextMenus",
   "tabs",
   "https://www.youtube.com/*",
   "https://youtube.com/*",
   "http://46.101.80.208:3210/*"
   ],
   "icons":{
      "512":"icon.png"
    }
}
