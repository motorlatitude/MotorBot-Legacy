{
  "manifest_version": 2,
  "name": "__MSG_extName__",
  "description": "This extension lets you add youtube videos to Motorbot",
  "version": "1.1.1",
  "version_name": "build 1.1.3~120716",
  "short_name": "MB",
  "default_locale": "en",
  "author": "Lennart Hase",
  "content_scripts": [
    {
      "matches": ["https://www.youtube.com/*", "https://youtube.com/*"],
      "js": ["jquery.min.js","ytb.js"],
      "run_at": "document_end"
    }
  ],
 "web_accessible_resources": [
     "icon_20x20.png"
   ],
  "permissions": [
   "activeTab",
   "contextMenus",
   "tabs",
   "https://www.youtube.com/*",
   "https://youtube.com/*"
   ],
   "icons":{
      "128": "motorbotIcon.png"
    }
}
