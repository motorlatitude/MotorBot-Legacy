body
html
  margin 0
  padding 0
  background #282c34
  color #fff
  font-family Helvetica Neue, helvetica, Lucida Grande, Arial, sans-serif
  font-size 13px
  font-weight 400

ul
  list-style none
  margin 0
  padding 0
  width 100%
  height 100%
  & li
    float left
    width 100%
    padding 10px
    &:nth-child(2n)
      background #2c323c
    &.titleRow
      font-weight bold
      border-bottom 1px solid rgba(255,255,255,0.2)
      position fixed
      top 0
      left 0
      right 0
      background #282c34
    &:nth-child(2)
      margin-top 37px
    & .item
      display inline-block
      width 2%
    & .title
      display inline-block
      width 48%
    & .videoId
      display inline-block
      width 10%
    & .time
      display inline-block
      width 20%
    & .status
      display inline-block
      width 20%
